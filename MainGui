	message2 = message2 .. "ðŸŸ£ Common Mega: " .. rarityCounts.commonMega .. "\n"
	message2 = message2 .. "\n"
	message2 = message2 .. "ðŸŸª Uncommon: " .. rarityCounts.uncommon .. "\n"
	message2 = message2 .. "ðŸŸ¢ Uncommon Neon: " .. rarityCounts.uncommonNeon .. "\n"
	message2 = message2 .. "ðŸŸ£ Uncommon Mega: " .. rarityCounts.uncommonMega .. "\n"
