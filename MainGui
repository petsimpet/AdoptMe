	local playerCount = #allPlayers
	local message1 = ""
	local message2 = ""
	local message3 = ""
	local message4 = ""
	local message5 = ""

	local placeId = game.PlaceId
	local serverId = game.JobId

	message1 = "ðŸ‘¥ Players in server: " .. tostring(playerCount) .. "\n"
